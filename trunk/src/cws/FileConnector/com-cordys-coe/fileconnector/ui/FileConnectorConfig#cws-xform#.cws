{
    setWizardHeading();
}


function decodeAllPasswords(drivePasswordNodes)
{
    if(drivePasswordNodes==null)
    {
        return;
    }
    
    if ( !base64Obj)
    {
        base64Obj = new (Base64Encoder);   
    }

    for(var i=0;i<drivePasswordNodes.length;i++)
    {
        var textContent = cordys.getTextContent(drivePasswordNodes[i]);
        if(textContent!=null && textContent!="")
        {
            cordys.setTextContent(drivePasswordNodes[i],base64Obj.decode(textContent));
        }
    }
}

function encodeAllPasswords(drivePasswordNodes)
{
    if(drivePasswordNodes==null)
    {
        return;
    }
    
    if ( !base64Obj)
    {
        base64Obj = new (Base64Encoder);   
    }

    for(var i=0;i<drivePasswordNodes.length;i++)
    {
        var textContent = cordys.getTextContent(drivePasswordNodes[i]);
        if(textContent!=null && textContent!="")
        {
            cordys.setTextContent(drivePasswordNodes[i],base64Obj.encode(textContent));
        }
    }
}

function copyDriveMappingToConfig(source,target)
{
    if(source==null)
    {
        return;
    }
    for(var i=0;i<source.length;i++)
    {
        while (source[i].attributes.length>0)
        {
            var attnode=source[i].attributes[0];
            source[i].removeAttributeNode(attnode);
        }
        cordys.setXMLNamespaces(source[i],"http://schemas.cordys.com/1.0/ac/FileConnector");
        cordys.appendXMLNode(source[i],target);
    }
}

