										<xforms:label>true</xforms:label>
										<xforms:value>true</xforms:value>
									</xforms:item>
									<xforms:item>
										<xforms:label>false</xforms:label>
										<xforms:value>false</xforms:value>
									</xforms:item>
									<xforms:action
										ev:event="xforms-value-changed">
										<xforms:chkUseSimpleXPath_Change/>
									</xforms:action>
								</xforms:select1>
