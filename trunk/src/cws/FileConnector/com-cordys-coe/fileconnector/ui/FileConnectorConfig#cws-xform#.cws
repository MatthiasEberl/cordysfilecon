				<![CDATA[var base64Obj;
var isFormLoad = true;
var ns= {"filecon": "http://schemas.cordys.com/1.0/ac/FileConnector"};
var gConfigurationNode;

function Form_InitDone(eventObject)
{
    application.importType("wcp.library.util.Base64Encoder");
    
    //Set Default values
    txtInputCharSet.setValue("ISO-8859-1");
    txtOutputCharSet.setValue("ISO-8859-1");
    txtReaderConfigFile.setValue("/Cordys/fileconnector/config/reader-config.xml");
    txtWriterConfigFile.setValue("/Cordys/fileconnector/config/writer-config.xml");
    txtConfigLocation.setValue("/Cordys/fileconnector/config/poller-config.xml");
    txtProcFolder.setValue("poller/processing");
    txtErrorFolder.setValue("poller/error");
    txtPollInterval.setValue("10");
    txtMinWorkerThreads.setValue("1");
    txtMaxWorkerThreads.setValue("10");
    
    disablePollerView();
    
    var appData = application.event.data;
    if(!application.container.isWizardContainer)
    {
       if(appData && appData.isDisabled)
       {
          application.disable();
       }
    }
    initialize();
    isFormLoad = false;
}

function raiseConfigurationChanges()
{
    if(!isFormLoad && !application.container.isWizardContainer)
        system.windows.servicepropertiesPage_SystemResourceManager.configurationChanged();
}

function chkEnabled_Change(eventObject)
{
    raiseConfigurationChanges();
    if(eventObject.srcElement.getValue()=="true")
    {
        enablePollerView();
    }
    else
    {
        disablePollerView();
    }
}

function enablePollerView()
{
    txtConfigLocation.enable();
    txtPollInterval.enable();
    txtProcFolder.enable();
    txtMinWorkerThreads.enable();
    txtMaxWorkerThreads.enable();
    txtErrorFolder.enable();
    txtDefaultUserDn.enable();
    txtAppProcFolder.enable();
}

function disablePollerView()
{
    txtConfigLocation.disable();
    txtPollInterval.disable();
    txtProcFolder.disable();
    txtMinWorkerThreads.disable();
    txtMaxWorkerThreads.disable();
    txtErrorFolder.disable();
    txtDefaultUserDn.disable();
    txtAppProcFolder.disable();
}

function fillInPropertyScreen(configurationNode)
{  
    var config = cordys.selectXMLNode(configurationNode,".//filecon:Configuration",ns);
    
    var readerConfigFile = cordys.selectXMLNode(config ,".//filecon:readerconfigfile",ns);
    var writerConfigFile = cordys.selectXMLNode(config ,".//filecon:writerconfigfile",ns);
    var inputCharSet = cordys.selectXMLNode(config ,".//filecon:readercharset",ns);
    var outputCharSet = cordys.selectXMLNode(config ,".//filecon:writercharset",ns);
    var includedDirectories = cordys.selectXMLNode(config ,".//filecon:included-directories",ns);
    var reloadConfiguration = cordys.selectXMLNode(config ,".//filecon:reload-configuration",ns);
    var useSimpleXPath = cordys.selectXMLNode(config ,".//filecon:use-simple-xpath",ns);
    var driveMappingNode = cordys.selectXMLNode(config ,".//filecon:drivemappings",ns);

    var pollerConfig = cordys.selectXMLNode(configurationNode,".//filecon:component[@name='Directory Poller']",ns);

    var pollerEnabled = cordys.selectXMLNode(pollerConfig ,".//filecon:enabled",ns);
    var pollerConfigLocation = cordys.selectXMLNode(pollerConfig ,".//filecon:configuration-file",ns);
    var pollerProcFolder = cordys.selectXMLNode(pollerConfig ,".//filecon:processing-folder",ns);
    var pollerAppProcFolder = cordys.selectXMLNode(pollerConfig ,".//filecon:app-processing-folder",ns);
    var pollerErrorFolder = cordys.selectXMLNode(pollerConfig ,".//filecon:error-folder",ns);
    var pollerPollInterval = cordys.selectXMLNode(pollerConfig ,".//filecon:poll-interval",ns);
    var pollerMinWorkerThreads = cordys.selectXMLNode(pollerConfig ,".//filecon:minConcurrentWorkers",ns);
    var pollerMaxWorkerThreads = cordys.selectXMLNode(pollerConfig ,".//filecon:maxConcurrentWorkers",ns);
    var pollerDefaultUserDn = cordys.selectXMLNode(pollerConfig ,".//filecon:defaultUserDn",ns);
    
    txtReaderConfigFile.setValue(cordys.getTextContent(readerConfigFile));
    txtWriterConfigFile.setValue(cordys.getTextContent(writerConfigFile));
    txtInputCharSet.setValue(cordys.getTextContent(inputCharSet));
    txtOutputCharSet.setValue(cordys.getTextContent(outputCharSet));
    txtIncludedDirectories.setValue(cordys.getTextContent(includedDirectories));
    chkReloadConfiguration.setValue(cordys.getTextContent(reloadConfiguration));
    chkUseSimpleXPath.setValue(cordys.getTextContent(useSimpleXPath));

    drivemapping.clear();
    if(driveMappingNode!=null)
    {
        decodeAllPasswords(cordys.selectXMLNodes(driveMappingNode,".//filecon:password",ns));
        drivemapping.putData(driveMappingNode,true);
    }
    else
    {
    }

    chkEnabled.setValue(cordys.getTextContent(pollerEnabled ));
    txtConfigLocation.setValue(cordys.getTextContent(pollerConfigLocation ));
    txtProcFolder.setValue(cordys.getTextContent(pollerProcFolder ));
    txtAppProcFolder.setValue(cordys.getTextContent(pollerAppProcFolder ));
    txtErrorFolder.setValue(cordys.getTextContent(pollerErrorFolder ));
    txtPollInterval.setValue(cordys.getTextContent(pollerPollInterval ));
    txtMinWorkerThreads.setValue(cordys.getTextContent(pollerMinWorkerThreads ));
    txtMaxWorkerThreads.setValue(cordys.getTextContent(pollerMaxWorkerThreads ));
    txtDefaultUserDn.setValue(cordys.getTextContent(pollerDefaultUserDn ));
}

function createConnectorConfiguration(configurationNode)
{
    var oldConfigNode = cordys.selectXMLNode(configurationNode,".//filecon:Configuration",ns);
    
    if(oldConfigNode!=null)
        oldConfigNode.parentNode.removeChild(oldConfigNode);
    
    var config = cordys.cloneXMLDocument(xmlConfiguration.XMLDocument).documentElement;
    
    var readerConfigFile = cordys.selectXMLNode(config ,".//filecon:readerconfigfile",ns);
    var writerConfigFile = cordys.selectXMLNode(config ,".//filecon:writerconfigfile",ns);
    var inputCharSet = cordys.selectXMLNode(config ,".//filecon:readercharset",ns);
    var outputCharSet = cordys.selectXMLNode(config ,".//filecon:writercharset",ns);
    var includedDirectories = cordys.selectXMLNode(config ,".//filecon:included-directories",ns);
    var reloadConfiguration = cordys.selectXMLNode(config ,".//filecon:reload-configuration",ns);
    var useSimpleXPath = cordys.selectXMLNode(config ,".//filecon:use-simple-xpath",ns);
    var driveMappingNode = cordys.selectXMLNode(config ,".//filecon:drivemappings",ns);

    var pollerConfig = cordys.cloneXMLDocument(xmlPollerConfiguration.XMLDocument).documentElement;

    var pollerEnabled = cordys.selectXMLNode(pollerConfig ,".//filecon:enabled",ns);
    var pollerConfigLocation = cordys.selectXMLNode(pollerConfig ,".//filecon:configuration-file",ns);
    var pollerProcFolder = cordys.selectXMLNode(pollerConfig ,".//filecon:processing-folder",ns);
    var pollerAppProcFolder = cordys.selectXMLNode(pollerConfig ,".//filecon:app-processing-folder",ns);
    var pollerErrorFolder = cordys.selectXMLNode(pollerConfig ,".//filecon:error-folder",ns);
    var pollerPollInterval = cordys.selectXMLNode(pollerConfig ,".//filecon:poll-interval",ns);
    var pollerMinWorkerThreads = cordys.selectXMLNode(pollerConfig ,".//filecon:minConcurrentWorkers",ns);
    var pollerMaxWorkerThreads = cordys.selectXMLNode(pollerConfig ,".//filecon:maxConcurrentWorkers",ns);
    var pollerDefaultUserDn = cordys.selectXMLNode(pollerConfig ,".//filecon:defaultUserDn",ns);
    
    cordys.setTextContent(readerConfigFile,txtReaderConfigFile.getValue());
    cordys.setTextContent(writerConfigFile ,txtWriterConfigFile.getValue());
    cordys.setTextContent(inputCharSet ,txtInputCharSet.getValue());
    cordys.setTextContent(outputCharSet ,txtOutputCharSet.getValue());
    cordys.setTextContent(includedDirectories ,txtIncludedDirectories.getValue());
    cordys.setTextContent(reloadConfiguration ,chkReloadConfiguration.getValue());
    cordys.setTextContent(useSimpleXPath ,chkUseSimpleXPath.getValue());
    
    var driveMappingDataNode = cordys.cloneXMLDocument(drivemapping.getData());
    
    encodeAllPasswords(cordys.selectXMLNodes(driveMappingDataNode ,".//*[local-name()='password']"));
    
    copyDriveMappingToConfig(cordys.selectXMLNodes(driveMappingDataNode ,".//*[local-name()='drivemapping']"), driveMappingNode);
    
    cordys.setTextContent(pollerEnabled,chkEnabled.getValue());
    cordys.setTextContent(pollerConfigLocation,txtConfigLocation.getValue());
    cordys.setTextContent(pollerProcFolder ,txtProcFolder.getValue());
    cordys.setTextContent(pollerAppProcFolder ,txtAppProcFolder.getValue());
    cordys.setTextContent(pollerErrorFolder ,txtErrorFolder.getValue());
    cordys.setTextContent(pollerPollInterval ,txtPollInterval.getValue());
    cordys.setTextContent(pollerMinWorkerThreads ,txtMinWorkerThreads.getValue());
    cordys.setTextContent(pollerMaxWorkerThreads ,txtMaxWorkerThreads.getValue());
    cordys.setTextContent(pollerDefaultUserDn ,txtDefaultUserDn.getValue());
    
    
    cordys.appendXMLNode(config ,configurationNode);
    cordys.appendXMLNode(pollerConfig ,configurationNode);
    
    return true;
    
}


function setWizardHeading() 
{
    if(application.container.isWizardContainer) 
    {
        var wizard =  application.container.srcApplication.container.applicationWindow.wizard;
        wizard.setHeading(localization_get("Provide Details for File Connector"));
     }
 } 
 
