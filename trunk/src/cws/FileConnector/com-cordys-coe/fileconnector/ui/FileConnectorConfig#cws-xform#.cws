
<XFormDesigntime
	xmlns=""
	xmlns:cws="http://schemas.cordys.com/cws/1.0"
	type="com.cordys.xforms.XFormDesigntime"
	typeVersion="005056C0-0008-11E0-F8FC-5E0A815B7FB8"
	RuntimeDocumentID="005056C0-0008-11E1-F460-865B74681B6E">
	<DocumentID>005056C0-0008-11E1-F460-865B7467FB6E</DocumentID>
	<Name>FileConnectorConfig</Name>
	<Description>FileConnectorConfig</Description>
	<CreatedBy>gkaviyarasu</CreatedBy>
	<CreationDate>2012-05-21T05:45:17.801</CreationDate>
	<Notes/>
	<XFormContent>
		<xformhtml
			xmlns="http://schemas.cordys.com/1.0/xform"
			xmlns:xforms="http://www.w3.org/2002/xforms/cr"
			xmlns:ev="http://www.w3.org/2001/xml-events"
			xmlns:wcpforms="http://schemas.cordys.com/wcp/xforms"
			xformsversion="2"
			key="com-cordys-coe/fileconnector/ui/FileConnectorConfig"
			focusatstartup="true"
			showNotify="true"
			showError="true">
			<xhead>
				<script
					src="/cordys/wcp/admin/behavior/applicationconnector.js"
				/>
				<xforms:model
					id="drivemapping"
					wcpforms:gateway="com.eibus.web.soap.Gateway.wcp"
					wcpforms:automatic="false"
					wcpforms:businessElement="drivemapping"
					singleTransaction="false"
					promptToSave="false"
					isConstraintValidation="false"
					isInitializationRequired="false"
					isApplyAccessControl="false"
					instanceSchema="false"
					wcpforms:nontransactional="true">
					<xforms:instance
						xmlns:xforms="http://www.w3.org/2002/xforms/cr"
						xmlns:wcpforms="http://schemas.cordys.com/wcp/xforms"
						wcpforms:timeout=""
						wcpforms:iteratorSize="50"
					/>
					<wcpforms:instanceschema/>
					<wcpforms:xpathmetainfo
						xmlns:wcpforms="http://schemas.cordys.com/wcp/xforms"
					/>
					<xforms:action
						ev:event="xforms-change">
						<xforms:drivemapping_Change/>
					</xforms:action>
				</xforms:model>
				<xml
					xmlns="">
					<xml
						id="xmlConfiguration">
						<Configuration
							xmlns="http://schemas.cordys.com/1.0/ac/FileConnector">
							<readerconfigfile/>
							<writerconfigfile/>
							<readercharset/>
							<writercharset/>
							<included-directories/>
							<reload-configuration/>
							<use-simple-xpath/>
							<drivemappings/>
						</Configuration>
					</xml>
					<xml
						id="xmlPollerConfiguration">
						<component
							xmlns="http://schemas.cordys.com/1.0/ac/FileConnector"
							name="Directory Poller">
							<enabled>true</enabled>
							<configuration-file/>
							<processing-folder/>
							<app-processing-folder/>
							<error-folder/>
							<poll-interval/>
							<minConcurrentWorkers/>
							<maxConcurrentWorkers/>
							<defaultUserDn/>
						</component>
					</xml>
				</xml>
			</xhead>
			<xbody>
				<xforms:group
